<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.ClubEntity" table="club" schema="leqi">
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="title">
            <column name="title" sql-type="text"/>
        </property>
        <property name="owner">
            <column name="owner" sql-type="text"/>
        </property>
        <property name="description">
            <column name="description" sql-type="text"/>
        </property>
        <property name="level">
            <column name="level" sql-type="text"/>
        </property>
        <property name="contact">
            <column name="contact" sql-type="text"/>
        </property>
        <property name="province">
            <column name="province" sql-type="text"/>
        </property>
        <property name="city">
            <column name="city" sql-type="text"/>
        </property>
        <property name="district">
            <column name="district" sql-type="text"/>
        </property>
        <property name="detail">
            <column name="detail" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>