<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.ClubMemberEntity" table="club_member" schema="leqi">
        <property name="userIdPerson">
            <column name="user_id_person" sql-type="int(11)"/>
        </property>
        <property name="userIdClub">
            <column name="user_id_club" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>